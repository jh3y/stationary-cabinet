.scene
  .stacked__wrapper
    .stacked
      .stacked__tray.tray
        .tray__side.tray__side--bottom
          +cuboid()(class="cuboid--tray")
        .tray__side.tray__side--left
          +cuboid()(class="cuboid--tray")
        .tray__side.tray__side--right
          +cuboid()(class="cuboid--tray")
      .stacked__stack.stack
        - let b = 0
        while b < 20
          +block(b)
          - b++