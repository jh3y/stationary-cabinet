mixin cuboid()
  .cuboid(class!=attributes.class)
    - let s = 0
    while s < 6
      .cuboid__side
      - s++

mixin block(delay)
  .stacked__block(style=`--delay: ${delay}; --index: ${delay};`)
    .block
      +cuboid()(class="cuboid--block")

